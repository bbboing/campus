<!--pages/addNumber/index.wxml-->
<van-cell-group>
  <van-field
    value="{{ username }}"
    required
    clearable
    label="学号"
    icon="question-o"
    placeholder="请输入学号"
    bind:input="changeNum"
  />

  <van-field
    value="{{ password }}"
    type="password"
    label="密码"
    placeholder="请输入密码"
    required
    border="{{ false }}"
    bind:input="changePwd"
  />
  <van-button round type="info" bind:click='addinfo'>提交</van-button>
  
</van-cell-group>
